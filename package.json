{
    "name": "@wxn0brp/vql",
    "version": "0.8.3",
    "main": "dist/index.js",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "tsc && tsc-alias",
        "test": "bun run src/tests/index.ts",
        "types": "bunx dts-bundle-generator src/types/vql.ts -o dist/vql.d.ts --external-inlines @wxn0brp/db-core"
    },
    "description": "VQL - Valthera Query Language",
    "homepage": "https://github.com/wxn0brP/VQL",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/VQL.git"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "vql",
        "valthera",
        "database",
        "orm",
        "api",
        "crud",
        "nosql",
        "query",
        "db",
        "query-language"
    ],
    "peerDependencies": {
        "@wxn0brp/db-core": ">=0.2.7",
        "@wxn0brp/falcon-frame": ">=0.5.3",
        "@wxn0brp/gate-warden": ">=0.5.0"
    },
    "peerDependenciesMeta": {
        "@wxn0brp/falcon-frame": {
            "optional": true
        },
        "@wxn0brp/db-core": {
            "optional": false
        },
        "@wxn0brp/gate-warden": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/node": "*",
        "@wxn0brp/db": "^0.40.2",
        "@wxn0brp/db-core": "^0.2.7",
        "@wxn0brp/falcon-frame": "^0.5.3",
        "@wxn0brp/gate-warden": "^0.5.0",
        "esbuild": "*",
        "tsc-alias": "*",
        "typescript": "*"
    },
    "dependencies": {
        "@wxn0brp/lucerna-log": "^0.2.2"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    }
}