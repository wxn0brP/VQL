{
    "name": "@wxn0brp/vql",
    "version": "0.3.5",
    "main": "dist/index.js",
    "author": "wxn0brP",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "tsc && tsc-alias",
        "test": "node dist/tests/index.js",
        "types": "dts-bundle-generator src/types/vql.ts -o dist/vql.d.ts --external-inlines @wxn0brp/db",
        "schema": "npm run types && node schema.js"
    },
    "description": "VQL - Valthera Query Language",
    "homepage": "https://github.com/wxn0brP/VQL",
    "repository": {
        "type": "git",
        "url": "https://github.com/wxn0brP/VQL.git"
    },
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "@wxn0brp/db": ">=0.8.1",
        "@wxn0brp/falcon-frame": ">=0.0.6"
    },
    "peerDependenciesMeta": {
        "@wxn0brp/falcon-frame": {
            "optional": true
        },
        "@wxn0brp/db": {
            "optional": false
        }
    },
    "devDependencies": {
        "@types/node": "^22.15.30",
        "@wxn0brp/db": "^0.8.1",
        "@wxn0brp/falcon-frame": "0.0.6",
        "dotenv": "^16.5.0",
        "source-map-support": "^0.5.21",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.7.3",
        "typescript-json-schema": "^0.65.1"
    },
    "dependencies": {
        "@wxn0brp/gate-warden": "^0.1.0",
        "@wxn0brp/lucerna-log": "^0.1.1",
        "@wxn0brp/wts-deep-merge": "github:wxn0brp/ts-shared#dist-deep-merge",
        "ajv": "^8.17.1",
        "ajv-formats": "^3.0.1",
        "js-yaml": "^4.1.0",
        "json5": "^2.2.3"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    }
}
